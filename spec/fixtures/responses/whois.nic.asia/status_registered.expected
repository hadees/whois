#domain
  should: %s == "cj7.asia"

#domain_id
  should: %s == "D93126-ASIA"


#referral_whois
  should: %s raise_error(Whois::PropertyNotSupported)

#referral_url
  should: %s raise_error(Whois::PropertyNotSupported)


#status
  should: %s == ["OK"]

#available?
  should: %s == false

#registered?
  should: %s == true


#created_on
  should: %s CLASS(time)
  should: %s == Time.parse("2008-01-15 11:28:02 UTC")

#updated_on
  should: %s CLASS(time)
  should: %s == Time.parse("2008-03-16 04:30:25 UTC")

#expires_on
  should: %s CLASS(time)
  should: %s == Time.parse("2010-01-15 11:28:02 UTC")


#registrar
  should: %s CLASS(registrar)
  should: %s.id           == "9998"
  should: %s.name         == "dotASIA R4-ASIA"
  should: %s.organization == "dotASIA R4-ASIA"
  should: %s.url          == nil

